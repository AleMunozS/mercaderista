// prisma/schema.prisma

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}


model Usuario {
  id         Int          @id @default(autoincrement())
  nombre     String
  email      String       @unique
  password   String
  roles      String

}



model OmOrderGridV {
  id               Int      @id
  order_nbr        String
  order_type       String
  status           Int
  priority         Int
  hold_flag        String?
  consignee_id     Int?
  currency_code    String?
  order_value      Float?
  incoterm_code    String?
  transport_mode   String?
  service_level    String?
  channel_code     String?
  ship_window_from DateTime?
  ship_window_to   DateTime?
  deliv_window_to  DateTime?
  create_date      DateTime
  line_count       Int?
  total_qty        Float?

  @@map("om_order_grid_v")
  @@ignore  // marca que no intente mutar la vista
}
